			}]
		},
		"tag": "ss"
	}, {
		"mux": {
			"enabled": false
		},
		"protocol": "vmess",
		"settings": {
			"vnext": [{
				"address": "xxxxxxxx",
				"port": 80,
				"users": [{
					"id": "xxxxxxxx",
					"alterId": 16,
					"security": "auto",
					"level": 0
				}]
			}]
		},
		"streamSettings": {
			"network": "tcp",
			"security": ""
