{
	"log": {
		"error": "/data/v2ray/run/error.log",
		"loglevel": "warning"
	},
	"inbounds": [{
		"port": 65534,
		"tag": "dns-in",
		"protocol": "dokodemo-door",
		"settings": {
			"port": 53,
			"address": "223.5.5.5",
			"network": "tcp,udp,tls"
		}
	}, {
		"port": 65535,
		"listen": "0.0.0.0",
		"tag": "proxy-inbound",
		"protocol": "dokodemo-door",
		"settings": {
			"network": "tcp,udp",
			"followRedirect": true
		},
		"sniffing": {
			"enabled": true,
			"destOverride": ["http", "tls"]
		}
	}],
	"outbounds": [{
		"protocol": "shadowsocks",
		"settings": {
			"servers": [{
				"address": "xxxxxxxx",  // Your shadowsocks address
				"method": "aes-256-gcm",
				"password": "xxxxxxxx",  // Your shadowsocks password
				"port": 80,  // Your shadowsocks port
			}]
		},
		"tag": "ss"
	}, {
		"protocol": "freedom",
		"settings": {},
		"tag": "cn"
	}, {
		"protocol": "dns",
		"tag": "dns-out"
	}],
	"policy": {
		"levels": {
			"0": {
				"bufferSize": 4,
				"connIdle": 600,
				"uplinkOnly": 0,
				"downlinkOnly": 0
			}
		}
	},
	"routing": {
		"domainStrategy": "IPIfNonMatch",
		"rules": [{
			"type": "field",
			"outboundTag": "dns-out",
			"inboundTag": ["dns-in"]
		}, {
			"type": "field",
			"outboundTag": "cn",
			"ip": ["geoip:cn", "geoip:gc"]
		}, {
			"type": "field",
			"outboundTag": "cn",
			"domain": ["geosite:cn", "geosite:gc"]
		}]
	},
	"dns": {
		"hosts": {
			"geosite:ad": "0.0.0.0"
		},
		"servers": ["223.5.5.5", "223.6.6.6"]
	}
}